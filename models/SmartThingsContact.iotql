CREATE MODEL SmartThingsContact WITH
    schema:name = "smart-things-contact",
    iot:facet = iot-facet:sensor,
    iot:facet = iot-facet:sensor.contact
ATTRIBUTE open WITH
    schema:name = "open",
    iot:purpose = iot-purpose:open,
    iot:type = iot:type.boolean,
    iot:sensor = true,
    iot:read = true
;
