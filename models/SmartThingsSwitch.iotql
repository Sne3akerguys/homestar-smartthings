CREATE MODEL SmartThingsSwitch WITH
    schema:name = "smart-things-switch",
    iot:facet = iot-facet:switch,
    iot:model = "https://raw.githubusercontent.com/dpjanes/homestar-smartthings/master/models/smart-things-switch.json"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean
;
