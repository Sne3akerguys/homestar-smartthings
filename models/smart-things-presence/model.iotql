CREATE MODEL SmartThingsPresence WITH
    schema:name = "smart-things-presence",
    iot:facet = iot-facet:sensor,
    iot:facet = iot-facet:sensor.presence,
    iot:model = "https://raw.githubusercontent.com/dpjanes/homestar-smartthings/master/models/smart-things-presence.json"
ATTRIBUTE presence WITH
    schema:name = "presence",
    iot:purpose = iot-purpose:presence,
    iot:type = iot:type.boolean,
    iot:read = true
;
